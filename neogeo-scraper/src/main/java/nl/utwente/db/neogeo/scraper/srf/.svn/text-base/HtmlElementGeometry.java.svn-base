package nl.utwente.db.neogeo.scraper.srf;

public class HtmlElementGeometry {
	
	public int 	x;
	public int	y;
	public int	height;
	public int	width;
	
	public HtmlElementGeometry(int x, int y, int height, int width) {
		this.x		= x;
		this.y		= y;
		this.height	= height;
		this.width	= width;
	}
	
	public int area() {
		return height * width;
	}
	
	public String toString() {
		return "HEG("+"x="+x+", y="+y+", height="+height+", width="+width+")";
	}
	
}
