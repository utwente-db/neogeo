package nl.utwente.db.neogeo.server;

import nl.utwente.db.neogeo.core.NeoGeoException;

public class InvalidRequestException extends NeoGeoException {
	private static final long serialVersionUID = 1L;

	public InvalidRequestException(String detailMessage) {
		super(detailMessage);
	}
	
	public InvalidRequestException(String detailMessage, Throwable cause) {
		super(detailMessage, cause);
	}
	
	public InvalidRequestException(Throwable cause) {
		super(cause);
	}
}
